{
  "extends": "astro/tsconfigs/strict",
  "include": [ ".astro/types.d.ts", "**/*" ],
  "exclude": [ "dist" ],
"compilerOptions": {
  "baseUrl": ".",
  "paths": {
    "@/*": [ "./src/*" ],
    "@scripts/*": [ "./src/scripts/*" ],
    "@styles/*": [ "./src/styles/*" ],
    "@layouts/*": [ "./src/layouts/*" ],
    "@components/*": [ "./src/components/*" ]
  },
}
}